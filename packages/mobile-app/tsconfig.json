{
  "compilerOptions": {
    "jsx": "react-native",
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "target": "ES2017",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["react-native", "expo"],
    "skipLibCheck": true
  },
  "include": ["src"]
}
